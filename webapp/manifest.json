{
	"_version": "1.8.0",
	"sap.app": {
		"id": "sap.m.PrintPOC",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"POC": {
				"uri": "localService/metadata.xml",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.m.PrintPOC.view.Screen1View",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.m.PrintPOC.i18n.i18n"
				}
			},
			"": {
				"dataSource": "POC"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sap.m.PrintPOC.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteScreen1View",
					"pattern": "RouteScreen1View",
					"target": [
						"TargetScreen1View"
					]
				},
				{
					"name": "Screen3View",
					"pattern": "Screen3View",
					"target": [
						"Screen3View"
					]
				},
				{
					"name": "Screen1View",
					"pattern": "Screen1View",
					"target": [
						"TargetScreen1View"
					]
				},
				{
					"name": "Screen2View",
					"pattern": "Screen2View",
					"target": [
						"Screen2View"
					]
				},
				{
					"name": "Screen4View",
					"pattern": "Screen4View",
					"target": [
						"Screen4View"
					]
				}
			],
			"targets": {
				"TargetScreen1View": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "Screen1View",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "sap.m.PrintPOC.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"Screen3View": {
					"viewType": "XML",
					"viewName": "Screen3View",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "sap.m.PrintPOC.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"Screen2View": {
					"viewType": "XML",
					"viewName": "Screen2View",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "sap.m.PrintPOC.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"Screen4": {
					"viewType": "XML",
					"viewName": "Screen4",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "sap.m.PrintPOC.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"Screen4View": {
					"viewType": "XML",
					"viewName": "Screen4View"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}