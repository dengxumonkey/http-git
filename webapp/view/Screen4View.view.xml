<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" controllerName="sap.m.PrintPOC.controller.Screen4View">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
				    <Table noDataText="Drop column list items here and columns in the area above" items="{path:'/eanSet',parameters:{select:'Style,EAN'}}" id="table0">
				    
				        <items>
				            <ColumnListItem type="Active" id="item10" counter="0">
				                <cells>
				                    <Text text="{EAN}" id="text2"/>
				                    <Text text="{Style}" id="text3"/>
				                </cells>
				            </ColumnListItem>
				        </items>
				        <columns>
				            <Column id="column0">
				                <header>
				                    <Label text="EAN" id="label10"/>
				                </header>
				            </Column>
				            <Column id="column1">
				                <header>
				                    <Label text="Style" id="label11"/>
				                </header>
				            </Column>
				           </columns></Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>